<p style="color: green"><%= notice %></p>

<% content_for :title, "Users" %>

<h1>Users</h1>

<div id="admin_users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", [:admin, user] %>
    </p>
  <% end %>
</div>

<%= link_to "New user", new_admin_user_path %>
